<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ThinMPm.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ThinMPm.Views.Page"
    FlyoutBehavior="Disabled"
    Title="ThinMPm">

    <ShellContent Route="MainPage" ContentTemplate="{DataTemplate views:MainPage}" />
    <ShellContent Route="AlbumsPage" ContentTemplate="{DataTemplate views:AlbumsPage}" />
    <ShellContent Route="AlbumDetailPage" ContentTemplate="{DataTemplate views:AlbumDetailPage}" />
</Shell>
